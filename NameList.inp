!
! GeometryName    Both single and double precision files are supported
! OutputFormat    = 'szplt', new data structure for tecplot.
!                   'plt', recommended.
!                   'dat'
$Global
    Aniso = .f., nStep = 10000, nSave = 1000, nAdaptStep = 500,
    Debug =  0 , CFL   = 0.5,   NRR   = .f.,  Chimera    = .f.,
    Limiter = 1, Restart = .f., nGeometry = 1, MeshOnly  = .t.,
    ! GeometryName  = './Data/11/bogie_8W',
    GeometryName  = './Data/sphere_500_',
    ! GeometryName  = './Data/pantograph',
    ! GeometryName  = './Data/zxj1',
    ! GeometryName  = './Data/LamborghiniB',
    GeometryFormat = 'facet',
    ! GeometryFormat = 'stl',
    OutputName = './Data/test',
    OutputFormat  = 'plt'
$END

! nCell
! DomainMin          minSize
! DomainMax          maxSize
! InitRefineLVL     It is also the maximum LVL
! AdaptRefineLVL
! cIntersectMethod  = 1 Ray-cast Painting Algorithm
!                     2 Ray-cast
!                     3 AABB Painting Algorithm
!                     4 AABB KDT
!                     5 AABB Traverse, obsolesced.
!                     6 Ray-cast Traverse, obsolesced.
! useKDT            = T slice surface data by KDT rules
!                     F slice surface data by x/y/z in turn
$Mesh
    InitRefineLVL = 6, AdaptRefineLVL  =  2,
    cIntersectMethod = 4, useKDT = .t.,
!bogie
!    nCell = 30,30,20,
!   DomainMin = -100.0, -100.0, -100.0,
!   DomainMax =  200.0,  200.0,  100.0,
