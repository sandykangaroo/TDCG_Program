!    DomainMax =  350.0,  100.0,  50.0,
!sphere
!	DomainMin = -2000.0, -2000.0, -2000.0,
!   DomainMax = 2000.0,  2000.0,  2000.0,
!pantograph
    ! nCell = 25, 10, 10,
	! DomainMin = -40000.0, -1700.0, 3000.0,
    ! DomainMax = -26000.0,  1700.0, 5500.0,
!sphere
!   nCell = 5,5,5,
!   DomainMin = -200, -200, -200,
!   DomainMax =  200,  200,  200,
!  nCell = 20,20,20,
!  DomainMin = -150.0, -150.0, -150,
!  DomainMax =  150.0,  150.0,  150.0,
    ! nCell = 20,20,20,
    ! DomainMin = -200, -200, -200,
    ! DomainMax =  200,  200,  200,
	 nCell = 20,20,20,
     DomainMin = -2000, -2000, -2000,
     DomainMax =  2000,  2000,  2000,
!wheel
!    nCell = 30,30,10,
!    DomainMin = -1., -0.5, -0.5,
!    DomainMax =  5.,  6.5,  2.,
!    DomainMin = 0, 0, 0,
!    DomainMax =  3.5,  6.0,  1.5,
!zxj2
!    nCell = 20,20,10,
!    DomainMin = -1200., -700.,  1700,
!    DomainMax =  1200.,  700.,  2400.,
$END

$NRRset
    NRRSeed = 1, NRRLength = 1, NRRTheta  =  1,
$END

$Inflow
    Alpha = 0.0, Beta = 0.0, Re = 2e+5, Rgas = 287.0,
